    "dataViewMappings": [
        {
            "conditions": [
                {
                    "Category": {
                        "max": 1
                    },
                    "Time": {
                        "max": 1
                    },
                    "Latitude": {
                        "max": 1
                    },
                    "Longitude": {
                        "max": 1
